    <BitmapImage x:Key="CrystalBall_Foreground_Top_Image_01" UriSource="/Community;component/Images/CystalBall/back_stars2.png"/>
    <BitmapImage x:Key="CrystalBall_Foreground_Bottom_Image_04" UriSource="/Community;component/Images/CystalBall/back_stars2.png"/>
