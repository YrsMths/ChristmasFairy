<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options" po:Freeze="True"
                    xmlns:Base="clr-namespace:Community.Themes.Basic">
    <!--Default颜色-->
    <Color x:Key="DefaultBorderBrushColor" po:Freeze="True">#FFC6E2FF</Color>
    <SolidColorBrush x:Key="DefaultBorderBrushSolidColorBrush" Color="{StaticResource DefaultBorderBrushColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="DefaultBackgroundColor" po:Freeze="True">#FFEBF4FF</Color>
    <SolidColorBrush x:Key="DefaultBackgroundSolidColorBrush" Color="{StaticResource DefaultBackgroundColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="DefaultBackgroundPressedColor" po:Freeze="True">#FF398DE6</Color>
    <SolidColorBrush x:Key="DefaultBackgroundPressedSolidColorBrush" Color="{StaticResource DefaultBackgroundPressedColor}" po:Freeze="True"></SolidColorBrush>

    <!--Primary颜色-->
    <Color x:Key="PrimaryNormalColor" po:Freeze="True">#4faeef</Color>
    <SolidColorBrush x:Key="PrimaryNormalSolidColorBrush" Color="{StaticResource PrimaryNormalColor}" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="WindowBorderBrushSolidColorBrush" Color="{StaticResource PrimaryNormalColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="PrimaryMouseOverColor" po:Freeze="True">#509dd1</Color>
    <SolidColorBrush x:Key="PrimaryMouseOverSolidColorBrush" Color="{StaticResource PrimaryMouseOverColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="PrimaryPressedColor" po:Freeze="True">#408ec3</Color>
    <SolidColorBrush x:Key="PrimaryPressedSolidColorBrush" Color="{StaticResource PrimaryPressedColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="PrimaryLighterColor" po:Freeze="True">#7ecbff</Color>
    <SolidColorBrush x:Key="PrimaryLighterSolidColorBrush" Color="{StaticResource PrimaryLighterColor}" po:Freeze="True"></SolidColorBrush>

    <!--Secondary颜色-->
    <Color x:Key="SecondaryNormalColor" po:Freeze="True">#ff99bb</Color>
    <SolidColorBrush x:Key="SecondaryNormalSolidColorBrush" Color="{StaticResource SecondaryNormalColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="SecondaryMouseOverColor" po:Freeze="True">#ff6e9f</Color>
    <SolidColorBrush x:Key="SecondaryMouseOverSolidColorBrush" Color="{StaticResource SecondaryMouseOverColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="SecondaryPressedColor" po:Freeze="True">#d47f9c</Color>
    <SolidColorBrush x:Key="SecondaryPressedSolidColorBrush" Color="{StaticResource SecondaryPressedColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="SecondaryLighterColor" po:Freeze="True">#ff568f</Color>
    <SolidColorBrush x:Key="SecondaryLighterSolidColorBrush" Color="{StaticResource SecondaryLighterColor}" po:Freeze="True"></SolidColorBrush>
    <!--Tertiary颜色-->
    <Color x:Key="TertiaryNormalColor" po:Freeze="True">#E6E61A</Color>
    <SolidColorBrush x:Key="TertiaryNormalSolidColorBrush" Color="{StaticResource TertiaryNormalColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="TertiaryMouseOverColor" po:Freeze="True">#E6E61A</Color>
    <SolidColorBrush x:Key="TertiaryMouseOverSolidColorBrush" Color="{StaticResource TertiaryMouseOverColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="TertiaryPressedColor" po:Freeze="True">#E6E61A</Color>
    <SolidColorBrush x:Key="TertiaryPressedSolidColorBrush" Color="{StaticResource TertiaryPressedColor}" po:Freeze="True"></SolidColorBrush>

    <Color x:Key="TertiaryLighterColor" po:Freeze="True">#E6E61A</Color>
    <SolidColorBrush x:Key="TertiaryLighterSolidColorBrush" Color="{StaticResource TertiaryLighterColor}" po:Freeze="True"></SolidColorBrush>
