    <Base:ImageCollection x:Key="Barbies">
        <BitmapImage UriSource="/Community;component/Images/Barbie/barbie_title.png"/>
        <BitmapImage UriSource="/Community;component/Images/Barbie/barbie.png"/>
    </Base:ImageCollection>
</ResourceDictionary>